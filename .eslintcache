ules":"48"},{"filePath":"89","messages":"90","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"48"},{"filePath":"91","messages":"92","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"48"},{"filePath":"93","messages":"94","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"95"},"/home/luizhse/workspace/minhas-financas-app/src/index.js",[],["96","97"],"/home/luizhse/workspace/minhas-financas-app/src/main/App.js",[],"/home/luizhse/workspace/minhas-financas-app/src/main/provedorAutenticacao.js",[],"/home/luizhse/workspace/minhas-financas-app/src/main/rotas.js",["98"],"import { HashRouter, Redirect, Route, Switch } from 'react-router-dom'\n\nimport { AuthConsumer } from '../main/provedorAutenticacao'\nimport CadastroLancamentos from '../views/lancamentos/cadastro-lancamentos'\nimport CadastroUsuario from '../views/cadastroUsuario'\nimport ConsultaLancamentos from '../views/lancamentos/consulta-lancamentos'\nimport Home from '../views/home'\nimport Login from '../views/login'\nimport React from 'react'\n\nfunction RotaAutenticada( { component: Component, isUsuarioAutenticado, ...props } ){\n    return <Route {...props} render = {(componentProps) => {\n       if(isUsuarioAutenticado){\n           return (\n                <Component {...componentProps} />\n           )\n       } else {\n           return (\n               <Redirect to={ { pathname: '/login', state: { from: componentProps.location} } } />\n           )\n       }\n    }} />\n\n}\n\nfunction Rotas(props){\n    return (\n        <HashRouter>\n            <Switch>\n                <Route path=\"/login\" component={Login} />\n                <Route path=\"/cadastro-usuarios\" component={CadastroUsuario} />\n                \n                <RotaAutenticada isUsuarioAutenticado={props.isUsuarioAutenticado} path=\"/\" component={Home} />\n                <RotaAutenticada isUsuarioAutenticado={props.isUsuarioAutenticado} path=\"/home\" component={Home} />\n                <RotaAutenticada isUsuarioAutenticado={props.isUsuarioAutenticado} path=\"/consulta-lancamentos\" component={ConsultaLancamentos} />\n                <RotaAutenticada isUsuarioAutenticado={props.isUsuarioAutenticado} path=\"/cadastro-lancamentos/:id?\" component={CadastroLancamentos} />\n            </Switch>\n        </HashRouter>\n    )\n}\n\nexport default () => (\n    <AuthConsumer>\n        {(context) => (\n            <Rotas isUsuarioAutenticado={context.isAutenticado} />\n        )}\n    </AuthConsumer>\n);\n\n\n","/home/luizhse/workspace/minhas-financas-app/src/components/navbar.js",["99"],"import { AuthConsumer } from '../main/provedorAutenticacao'\nimport NavbarItem from './navbaritem';\nimport React from 'react';\n\nfunction Navbar(props){\n    return (\n        <div className=\"navbar navbar-expand-lg fixed-top navbar-dark bg-primary\">\n        <div className=\"container\">\n            <a href=\"#/home\" className=\"navbar-brand\">Minhas Finanças</a>\n            <button className=\"navbar-toggler\" type=\"button\" \n            data-toggle=\"collapse\" data-target=\"#navbarResponsive\" \n            aria-controls=\"navbarResponsive\" aria-expanded=\"false\" \n            aria-label=\"Toggle navigation\">\n            <span className=\"navbar-toggler-icon\">\n                <NavbarItem render={props.isUsuarioAutenticado} href=\"#/home\" label=\"Home\" />\n                <NavbarItem render={props.isUsuarioAutenticado} href=\"#/cadastro-usuarios\" label=\"Usuários\" />\n                <NavbarItem render={props.isUsuarioAutenticado} href=\"#/consulta-lancamentos\" label=\"Lançamentos\" />\n                <NavbarItem render={props.isUsuarioAutenticado} onClick={props.deslogar} href=\"#/login\" label=\"Sair\" />\n            </span>\n            </button>\n            <div className=\"collapse navbar-collapse\" id=\"navbarResponsive\">\n            <ul className=\"navbar-nav\">\n                <NavbarItem render={props.isUsuarioAutenticado} href=\"#/home\" label=\"Home\" />\n                <NavbarItem render={props.isUsuarioAutenticado} href=\"#/cadastro-usuarios\" label=\"Usuários\" />\n                <NavbarItem render={props.isUsuarioAutenticado} href=\"#/consulta-lancamentos\" label=\"Lançamentos\" />\n                <NavbarItem render={props.isUsuarioAutenticado} onClick={props.deslogar} href=\"#/login\" label=\"Sair\" />\n            </ul>\n            </div>\n        </div>\n        </div>\n    )\n}\n\nexport default () => (\n    <AuthConsumer>\n        {(context) => (\n            <Navbar isUsuarioAutenticado={context.isAutenticado} deslogar={context.encerrarSessao}/>\n        )}\n    </AuthConsumer>\n);","/home/luizhse/workspace/minhas-financas-app/src/components/navbaritem.js",[],"/home/luizhse/workspace/minhas-financas-app/src/views/cadastroUsuario.js",[],"/home/luizhse/workspace/minhas-financas-app/src/views/home.js",[],"/home/luizhse/workspace/minhas-financas-app/src/views/login.js",[],"/home/luizhse/workspace/minhas-financas-app/src/views/lancamentos/cadastro-lancamentos.js",[],"/home/luizhse/workspace/minhas-financas-app/src/views/lancamentos/consulta-lancamentos.js",[],"/home/luizhse/workspace/minhas-financas-app/src/app/service/authService.js",[],"/home/luizhse/workspace/minhas-financas-app/src/components/card.js",[],"/home/luizhse/workspace/minhas-financas-app/src/components/form-group.js",[],"/home/luizhse/workspace/minhas-financas-app/src/components/toastr.js",[],"/home/luizhse/workspace/minhas-financas-app/src/app/service/usuarioService.js",[],"/home/luizhse/workspace/minhas-financas-app/src/app/service/lancamentoService.js",[],"/home/luizhse/workspace/minhas-financas-app/src/components/selectMenu.js",["100"],"import React from 'react'\n\nexport default (props) => {\n    \n    const options = props.lista.map( (option, index) => {\n        return (\n            <option key={index} value={option.value}>{option.label}</option>\n        )\n    })\n\n    return (\n        <select {...props}>\n            {options}\n        </select>\n    )\n}","/home/luizhse/workspace/minhas-financas-app/src/views/lancamentos/lancamentosTable.js",["101"],"import React from 'react'\nimport currencyFormatter from 'currency-formatter'\n\nexport default props => {\n\n    const rows = props.lancamentos.map( lancamento => {\n        return(\n            <tr key={lancamento.id}>\n                <td>{lancamento.descricao}</td>\n                <td>{currencyFormatter.format(lancamento.valor, {locale: 'pt-BR'})}</td>\n                <td>{lancamento.tipo}</td>\n                <td>{lancamento.mes}</td>\n                <td>{lancamento.status}</td>\n                <td>\n                    <button className=\"btn btn-success\" title=\"Efetivar\"\n                            disabled={ lancamento.status !== 'PENDENTE' }\n                            onClick={ e => props.alterarStatus(lancamento, 'EFETIVADO')}\n                            type=\"button\">\n                           <i className=\"pi pi-check\"></i>\n                    </button>\n\n                    <button className=\"btn btn-warning\" title=\"Cancelar\"\n                            disabled={ lancamento.status !== 'PENDENTE' }\n                            onClick={ e => props.alterarStatus(lancamento, 'CANCELADO')}\n                            type=\"button\">\n                            <i className=\"pi pi-times\"></i>\n                    </button>\n                    \n                    <button type=\"button\" className=\"btn btn-primary\" title=\"Editar\"\n                    onClick={e => props.editarAction(lancamento.id)}>\n                        <i className=\"pi pi-pencil\"></i>\n                    </button>\n                    \n                    <button type=\"button\" className=\"btn btn-danger\" title=\"Excluir\"\n                    onClick={e => props.deletarAction(lancamento)}>\n                       <i className=\"pi pi-trash\"></i>\n                    </button>\n                </td>\n            </tr>\n        )\n    })\n\n    return(\n        <table className=\"table table-hover\">\n            <thead>\n                <tr>\n                    <th scope=\"col\">Descrição</th>\n                    <th scope=\"col\">Valor</th>\n                    <th scope=\"col\">Tipo</th>\n                    <th scope=\"col\">Mês</th>\n                    <th scope=\"col\">Situação</th>\n                    <th scope=\"col\">Ações</th>\n                </tr>\n            </thead>\n            <tbody>\n                {rows}\n            </tbody>\n        </table>\n    )\n}","/home/luizhse/workspace/minhas-financas-app/src/app/service/localstorageService.js",[],"/home/luizhse/workspace/minhas-financas-app/src/app/apiservice.js",[],"/home/luizhse/workspace/minhas-financas-app/src/app/exception/erroValidacao.js",[],["102","103"],{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","severity":1,"message":"109","line":42,"column":1,"nodeType":"110","endLine":48,"endColumn":3},{"ruleId":"108","severity":1,"message":"109","line":34,"column":1,"nodeType":"110","endLine":40,"endColumn":3},{"ruleId":"108","severity":1,"message":"109","line":3,"column":1,"nodeType":"110","endLine":16,"endColumn":2},{"ruleId":"108","severity":1,"message":"109","line":4,"column":1,"nodeType":"110","endLine":60,"endColumn":2},{"ruleId":"104","replacedBy":"111"},{"ruleId":"106","replacedBy":"112"},"no-native-reassign",["113"],"no-negated-in-lhs",["114"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration",["113"],["114"],"no-global-assign","no-unsafe-negation"]